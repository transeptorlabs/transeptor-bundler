{
  "name": "transeptor-bundler",
  "private": true,
  "version": "0.13.1-alpha.0",
  "description": "Transeptor is a high-performance, modular ERC-4337 bundler designed to make smart accounts scalable and usable on Ethereum",
  "author": {
    "name": "Transeptor Labs",
    "email": "info@transeptorlabs.io",
    "url": "https://transeptorlabs.io"
  },
  "license": "GNU",
  "homepage": "https://github.com/transeptorlabs/transeptor-bundler#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/transeptorlabs/transeptor-bundler.git"
  },
  "bugs": {
    "url": "https://github.com/transeptorlabs/transeptor-bundler/issues"
  },
  "type": "module",
  "bin": {
    "transeptor": "./dist/cli.mjs"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "ls:cmd": "tsx ./src/cli.ts --help",
    "type-check": "tsc --noEmit",
    "build": "yarn type-check && rm -rf dist && pkgroll --target=node22.14.0 --minify",
    "build:contracts": "cd contracts && forge build",
    "build:typedocs": "typedoc",
    "dev": "tsx watch ./src/cli.ts --httpApi web3,eth,debug --txMode base | pino-pretty",
    "dev:unsafe": "tsx watch ./src/cli.ts --httpApi web3,eth,debug --txMode base --unsafe | pino-pretty",
    "dev:metrics": "tsx watch ./src/cli.ts --httpApi web3,eth,debug --metrics --txMode base | pino-pretty",
    "dev:auditTrail": "tsx watch ./src/cli.ts --httpApi web3,eth,debug --txMode base --auditTrail true | pino-pretty",
    "start": "tsx ./src/cli.ts --httpApi web3,eth,debug --txMode base | pino-pretty",
    "start:prod": "yarn build && node dist/cli.mjs --httpApi web3,eth,debug --txMode base | pino-pretty",
    "test": "yarn type-check && vitest run --config ./vitest.config.ts --coverage",
    "test:dev": "vitest --config ./vitest.config.ts",
    "lint": "eslint . --ext ts",
    "lint:fix": "yarn lint --fix",
    "send-op": "tsx ./e2e/send-userop.e2e.ts | pino-pretty",
    "test:mempool-race": "tsx ./e2e/data-race-test.ts",
    "print-dev_accounts": "tsx ./e2e/print-dev-accounts.ts",
    "///cK": "________________________DOCKER CONTAINERS______________________________",
    "start:eth-dev": "./eth-node",
    "build:image": "docker build -t bundler-typescript:v-local .",
    "start:image": "docker compose up -d bundler-typescript-local",
    "stop:image": "docker compose down",
    "influxdb": "docker compose up influxdb",
    "grafana": "docker compose up grafana"
  },
  "dependencies": {
    "@chainsafe/libp2p-noise": "^12.0.2",
    "@ethereumjs/common": "^10.0.0",
    "@ethereumjs/tx": "^10.0.0",
    "@ethereumjs/util": "^10.0.0",
    "@influxdata/influxdb-client": "1.33.2",
    "@libp2p/mplex": "^8.0.4",
    "@libp2p/tcp": "^7.0.3",
    "@multiformats/multiaddr": "^12.1.5",
    "async-mutex": "0.4.0",
    "commander": "10.0.1",
    "cors": "^2.8.5",
    "dotenv": "16.0.3",
    "ethereumjs-util": "^7.1.0",
    "ethers": "^6.13.4",
    "express": "^4.21.2",
    "helmet": "^7.1.0",
    "libp2p": "^0.45.9",
    "os-utils": "0.0.14",
    "pino": "^8.14.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "18.16.3",
    "@types/os-utils": "^0.0.4",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "@vitest/coverage-istanbul": "3.0.5",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.2.2",
    "eslint-plugin-prettier": "^5.2.1",
    "pino-pretty": "^10.0.0",
    "pkgroll": "^2.12.1",
    "prettier": "^3.3.3",
    "tsx": "^4.19.3",
    "typedoc": "^0.26.11",
    "typescript": "5.0.4",
    "vite": "^5.4.19",
    "vitest": "^3.0.5"
  },
  "packageManager": "yarn@4.7.0",
  "engines": {
    "node": ">=22.14.0"
  }
}
